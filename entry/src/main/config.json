{
  "app": {
    "bundleName": "com.blikoon.qrcodescannerlibrary",
    "vendor": "leeshy",
    "version": {
      "code": 1000000,
      "name": "1.0"
    }
  },
  "deviceConfig": {
    "default": {
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "package": "com.blikoon.youreading",
    "name": ".MyApplication",
    "mainAbility": "com.blikoon.youreading.MainAbility",
    "deviceType": [
      "phone"
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.DISTRIBUTED_DATASYNC",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.BIND_REMOTEVIEWS",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.servicebus.ACCESS_SERVICE",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.INTERNET",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.CAMERA",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.RECORD_AUDIO",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.MICROPHONE",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.SYSTEM_FLOAT_WINDOW",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.SYSTEM_OVERLAY_WINDOW",
        "reason": "reason",
        "usedScene": {
          "ability": [
            "com.blikoon.qrcodescannerlibrary"
          ],
          "when": "always"
        }
      },
      {
        "name": "ohos.permission.GET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.SET_NETWORK_INFO"
      }
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree":true
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "portrait",
        "name": "com.blikoon.youreading.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData":{
          "customizeData":[
            {
              "name": "hwc-theme",
              "extra": "",
              "value": "androidhwext:style/Theme.Emui.Light.NoTitleBar"
            }
          ]
        }
      },
      {
        "name": "com.blikoon.youreading.LocalDBAbility",
        "icon": "$media:icon",
        "description": "$string:localdbability_description",
        "type": "data",
        "uri": "dataability://com.blikoon.qrcodescannerlibrary.LocalDBAbility"
      }
    ],
    "defPermissions": [
      {
        "name": "com.blikoon.qrcodescannerlibrary.DataAbilityShellProvider.PROVIDER"
      }
    ]
  }
}